<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="sling:Folder">
	<widgetLib
			jcr:primaryType="sling:Folder"
			sling:resourceType="composum/nodes/commons/clientlib"
			jcr:description="clientlib for the AI extension to widgets in Pages"
			category="[composum.components.widgets]"
			order="{Long}1000">
		<css jcr:primaryType="sling:OrderedFolder"
		     embed="[
	                    composum/pages/options/ai/css/widgetextensions.css,
	                    composum/pages/options/ai/css/dialogs.css
	                ]"/>
		<js jcr:primaryType="sling:OrderedFolder"
		    embed="[
	                    composum/pages/options/ai/js/ai.js,
	                    composum/pages/options/ai/js/create.js,
	                    composum/pages/options/ai/js/translate.js,
	                    composum/pages/options/ai/js/categorize.js
	                ]">
		</js>
	</widgetLib>
	<pagesLib
			jcr:primaryType="sling:Folder"
			sling:resourceType="composum/nodes/commons/clientlib"
			jcr:description="clientlib for the AI extension to Pages right side bar"
			category="[composum.pages.edit.frame]"
						order="{Long}1000">
		<css jcr:primaryType="sling:OrderedFolder"
		     embed="[
	                    composum/pages/options/ai/css/sidebar.css
	                ]"/>
		<js jcr:primaryType="sling:OrderedFolder"
		    embed="[
	                    composum/pages/options/ai/js/sidebar.js
	                ]">
		</js>
	</pagesLib>
</jcr:root>
